{
  "easycom": {
    "autoscan": true,
    "custom": {
      // uni-ui 规则如下配置
      "^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue"
    }
  },
  "pages": [ // pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
    {
      "path": "pages/index/index"
    },
    {
      "path": "pages/manage/selectManage"
    },
    {
      "path": "pages/account/account"
    },
    {
      "path": "pages/summary/summary"
    },
    {
      "path": "pages/manage/dataManage"
    }

  ],
  "globalStyle": {
    "navigationStyle": "custom",
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "",
    "navigationBarBackgroundColor": "#fbfffb",
    "backgroundColor": "#fbfffb"
  },
  "tabBar": {
    "color": "#282b29",
    "selectedColor": "#3cc51f",
    "borderStyle": "black",
    "backgroundColor": "#fbfffb",
    "list": [{
      "pagePath": "pages/index/index",
      "iconPath": "static/img/mdi--home.png",
      "selectedIconPath": "static/img/mdi--home-active.png",
      "text": "主页"
    }, {
      "pagePath": "pages/summary/summary",
      "iconPath": "static/img/mdi--diary.png",
      "selectedIconPath": "static/img/mdi--diary--active.png",
      "text": "统计"
    }, {
      "pagePath": "pages/account/account",
      "iconPath": "static/img/mdi--account.png",
      "selectedIconPath": "static/img/mdi--account-active.png",
      "text": "个人中心"
    }]
  }
}
